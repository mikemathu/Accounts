                                                                <option value=1>Acknowledged</option>
                                                                <option value=2>All</option>
                                                            </select>
                                                        </div>
                                                        <div class=form-group>
                                                            <label for=dateFrom>
                                                                <small>Between</small>
                                                            </label>
                                                            <input type=date class="form-control input-sm" id=dateFrom required>
                                                        </div>
                                                        <div class=form-group>
                                                            <label for=dateTo>
                                                                <small>And</small>
                                                            </label>
