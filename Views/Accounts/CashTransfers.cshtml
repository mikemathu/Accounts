<div class=row>
                                    <div class="panel panel-default">
                                        <div class=panel-heading>
                                            <h5 class=panel-title>Cash Transfer Details</h5>
                                        </div>
                                        <div class=panel-body>
                                            <div class=row>
                                                <form method=post id=CashTransferForm>
                                                    <div class=col-sm-4>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Source Sub-Account</small>
                                                            </label>
                                                            <select class="form-control input-sm" id=SrcSubAccount required></select>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>
                                                                    Current Balance: <span class=text-info id=txtsrcBal></span>
                                                                </small>
                                                            </label>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Destination Sub-Account</small>
                                                            </label>
                                                            <select class="form-control input-sm" id=DestSubAccount required></select>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>
                                                                    Current Balance: <span class=text-info id=txtdestBal></span>
                                                                </small>
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class=col-sm-4>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Description</small>
                                                            </label>
                                                            <input class="form-control input-sm" id=Description required>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Amount</small>
                                                            </label>
                                                            <input type=number step=any class="form-control input-sm" id=Amount required>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Amount in words</small>
                                                            </label>
                                                            <input class="form-control input-sm" id=AmountInWords required>
                                                        </div>
                                                    </div>
                                                    <div class=col-sm-4>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Received By</small>
                                                            </label>
                                                            <input class="form-control input-sm" id=ReceivedBy required>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>DateTime Transfered</small>
                                                            </label>
                                                            <input type=datetime-local step=1 class="form-control input-sm" id=DateTimeTransfered required>
                                                        </div>
                                                        <div id=divShift hidden>
                                                            <div class=form-group>
                                                                <label>
                                                                    <small>Cashier Shift</small>
                                                                </label>
                                                                <select class="form-control input-sm" id=SysUserID></select>
                                                            </div>
                                                        </div>
                                                        <button class="btn btn-primary btn-xs pull-right ladda-button" data-style=expand-right data-size=s id=btnTransfer>Transfer</button>
                                                    </div>
